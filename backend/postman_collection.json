{
  "info": {
    "name": "Cloud Storage Sync API",
    "description": "API collection for Cloud Storage Sync Tool",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Authentication",
      "item": [
        {
          "name": "Register User",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"user@example.com\",\n  \"password\": \"securepassword123\",\n  \"full_name\": \"John Doe\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/auth/register",
              "host": ["{{base_url}}"],
              "path": ["auth", "register"]
            }
          }
        },
        {
          "name": "Login",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"user@example.com\",\n  \"password\": \"securepassword123\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/auth/login",
              "host": ["{{base_url}}"],
              "path": ["auth", "login"]
            }
          }
        },
        {
          "name": "Start Google OAuth",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url}}/auth/google/start?user_id=1",
              "host": ["{{base_url}}"],
              "path": ["auth", "google", "start"],
              "query": [
                {
                  "key": "user_id",
                  "value": "1"
                }
              ]
            }
          }
        },
        {
          "name": "Check Google Status",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url}}/auth/google/status/1",
              "host": ["{{base_url}}"],
              "path": ["auth", "google", "status", "1"]
            }
          }
        }
      ]
    },
    {
      "name": "File Upload",
      "item": [
        {
          "name": "Upload File",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{access_token}}"
              }
            ],
            "body": {
              "mode": "formdata",
              "formdata": [
                {
                  "key": "file",
                  "type": "file",
                  "src": "/path/to/your/file.pdf"
                },
                {
                  "key": "sync_google",
                  "value": "true",
                  "type": "text"
                },
                {
                  "key": "sync_azure",
                  "value": "false",
                  "type": "text"
                }
              ]
            },
            "url": {
              "raw": "{{base_url}}/api/v1/upload",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "upload"]
            }
          }
        },
        {
          "name": "Get Sync Status",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{access_token}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/v1/status/:job_id",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "status", ":job_id"],
              "variable": [
                {
                  "key": "job_id",
                  "value": ""
                }
              ]
            }
          }
        }
      ]
    },
    {
      "name": "Files",
      "item": [
        {
          "name": "List Files",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{access_token}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/v1/files?limit=50&offset=0",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "files"],
              "query": [
                {
                  "key": "limit",
                  "value": "50"
                },
                {
                  "key": "offset",
                  "value": "0"
                },
                {
                  "key": "status",
                  "value": "completed",
                  "disabled": true
                }
              ]
            }
          }
        },
        {
          "name": "Get File Info",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{access_token}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/v1/files/:file_id",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "files", ":file_id"],
              "variable": [
                {
                  "key": "file_id",
                  "value": "1"
                }
              ]
            }
          }
        },
        {
          "name": "Download File (Local)",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{access_token}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/v1/files/:file_id?download=true&from_storage=local",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "files", ":file_id"],
              "query": [
                {
                  "key": "download",
                  "value": "true"
                },
                {
                  "key": "from_storage",
                  "value": "local"
                }
              ],
              "variable": [
                {
                  "key": "file_id",
                  "value": "1"
                }
              ]
            }
          }
        },
        {
          "name": "Delete File",
          "request": {
            "method": "DELETE",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{access_token}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/v1/files/:file_id?delete_from_cloud=true",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "files", ":file_id"],
              "query": [
                {
                  "key": "delete_from_cloud",
                  "value": "true"
                }
              ],
              "variable": [
                {
                  "key": "file_id",
                  "value": "1"
                }
              ]
            }
          }
        }
      ]
    },
    {
      "name": "Conflicts",
      "item": [
        {
          "name": "List Conflicts",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{access_token}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/v1/conflicts?resolved=false",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "conflicts"],
              "query": [
                {
                  "key": "resolved",
                  "value": "false"
                }
              ]
            }
          }
        },
        {
          "name": "Get Conflict Details",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{access_token}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/v1/conflicts/:conflict_id",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "conflicts", ":conflict_id"],
              "variable": [
                {
                  "key": "conflict_id",
                  "value": "1"
                }
              ]
            }
          }
        },
        {
          "name": "Resolve Conflict",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{access_token}}"
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"policy\": \"last-write\",\n  \"notes\": \"Resolved by user\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/v1/conflicts/:conflict_id/resolve",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "conflicts", ":conflict_id", "resolve"],
              "variable": [
                {
                  "key": "conflict_id",
                  "value": "1"
                }
              ]
            }
          }
        }
      ]
    },
    {
      "name": "Health Check",
      "request": {
        "method": "GET",
        "url": {
          "raw": "{{base_url}}/health",
          "host": ["{{base_url}}"],
          "path": ["health"]
        }
      }
    }
  ],
  "variable": [
    {
      "key": "base_url",
      "value": "http://localhost:8000"
    },
    {
      "key": "access_token",
      "value": ""
    }
  ]
}
